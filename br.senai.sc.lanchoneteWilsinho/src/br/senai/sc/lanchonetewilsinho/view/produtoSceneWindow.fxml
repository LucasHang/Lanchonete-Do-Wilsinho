      </ImageView>
      <Button fx:id="btnCadastrar" layoutX="908.0" layoutY="476.0" mnemonicParsing="false" onAction="#btnCadastrarOnAction" prefHeight="53.0" prefWidth="113.0" text="Cadastrar" textFill="WHITE">
