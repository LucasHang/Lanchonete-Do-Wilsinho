      <Button fx:id="btnCadastrarProduto" layoutX="326.0" layoutY="627.0" mnemonicParsing="false" onAction="#btnCadastrarProdutoOnAction" prefHeight="65.0" prefWidth="134.0" text="Cadastrar &#10;Produto" textAlignment="CENTER" textFill="WHITE">
